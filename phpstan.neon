includes:
    - vendor/phpstan/phpstan-phpunit/extension.neon
    - vendor/phpstan/phpstan-phpunit/rules.neon

parameters:
    level: 8
    bootstrapFiles:
        - src/constants.php
    paths:
        - 'src'
        - 'tests'
        - 'benchmarks'
    excludes_analyse:
        - src/Backends/Amp.php
        - src/Graph/Trees/BallTree.php
        - src/Graph/Trees/ITree.php
        - src/Graph/Trees/KDTree.php
